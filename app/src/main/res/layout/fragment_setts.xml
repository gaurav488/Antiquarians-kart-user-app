<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Setts"
    android:orientation="vertical"
    android:background="#F8F8F8"
    >
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="7dp"
        app:cardCornerRadius="6dp"
        app:cardMaxElevation="1dp"
        app:cardElevation="1dp"
        android:id="@+id/About_AQs_account_card"
        app:cardPreventCornerOverlap="true"
        app:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <TextView
                android:layout_marginStart="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="About AQs Kart"
                android:textSize="15dp"
                android:id="@+id/About_AQs_text"
                android:textColor="#000"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:textAlignment="center"
              />
            <ImageView
                android:id="@+id/right_About_AQs_icon"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerVertical="true"
                android:src="@drawable/right_angle_setting"
                android:layout_alignParentRight="true"/>
        </RelativeLayout>

    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView

        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="7dp"
        app:cardCornerRadius="6dp"
        app:cardMaxElevation="1dp"
        app:cardElevation="1dp"
        android:id="@+id/About_AQs_contact_us"
        app:cardPreventCornerOverlap="true"
        app:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <TextView
                android:layout_marginStart="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Contact Us"
                android:textSize="15dp"
                android:id="@+id/About_AQs_contact_us_text"
                android:textColor="#000"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:textAlignment="center"
                />
            <ImageView
                android:id="@+id/About_AQs_contact_us_icon"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerVertical="true"
                android:src="@drawable/right_angle_setting"
                android:layout_alignParentRight="true"/>
        </RelativeLayout>

    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="7dp"
        app:cardCornerRadius="6dp"
        app:cardMaxElevation="1dp"
        app:cardElevation="1dp"
        android:id="@+id/About_AQs_account_terms_conditions"
        app:cardPreventCornerOverlap="true"
        app:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/terms_condition"
                android:textSize="15dp"
                android:id="@+id/terms_conditions_text"
                android:layout_marginStart="10dp"
                android:textColor="#000"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:textAlignment="center"
                />
            <ImageView
                android:id="@+id/right_terms_conditions_icon"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerVertical="true"
                android:src="@drawable/right_angle_setting"
                android:layout_alignParentRight="true"/>
        </RelativeLayout>

    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="7dp"
        app:cardCornerRadius="6dp"
        app:cardMaxElevation="1dp"
        app:cardElevation="1dp"
        android:id="@+id/privacy_policy_account_card"
        app:cardPreventCornerOverlap="true"
        app:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <TextView
                android:layout_marginStart="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Privacy Policy"
                android:textSize="15dp"
                android:id="@+id/privacy_policy_text"
                android:textColor="#000"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:textAlignment="center"
                />
            <ImageView
                android:id="@+id/right_privacy_policy_icon"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerVertical="true"
                android:src="@drawable/right_angle_setting"
                android:layout_alignParentRight="true"/>
        </RelativeLayout>

    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="7dp"
        app:cardCornerRadius="6dp"
        app:cardMaxElevation="1dp"
        app:cardElevation="1dp"
        android:id="@+id/copyright_usage_account_card"
        app:cardPreventCornerOverlap="true"
        app:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/copyright_usage"
                android:textSize="15dp"
                android:id="@+id/copyright_usage_text"
                android:textColor="#000"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:textAlignment="center"
                android:layout_marginStart="10dp"
                />
            <ImageView
                android:id="@+id/right_copyright_usage_icon"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerVertical="true"
                android:src="@drawable/right_angle_setting"
                android:layout_alignParentRight="true"/>
        </RelativeLayout>

    </androidx.cardview.widget.CardView>
</LinearLayout>